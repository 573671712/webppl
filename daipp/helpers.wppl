var testWithinTol = function(actual, expected, tol) {
  var msg = 'Expected: ' + expected + ', actual: ' + actual;
  console.log(Math.abs(actual - expected) < tol ? 'PASS' : 'FAIL', '|', msg);
};

var std = function(erp, mean) {
  return expectation(erp, function(x) {
    return Math.pow(x - mean, 2);
  });
};
